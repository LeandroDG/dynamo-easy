{
  "rules": {
    "no-angle-bracket-type-assertion": false,
    "max-line-length": [
      true,
      170
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "object-literal-sort-keys": false,
    "member-access": [
      true,
      "no-public"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ]
  },
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ]
}
